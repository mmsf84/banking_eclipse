{
	"name" : "bankaccount",
	"description" : "BankAccount",
	"keywords" : [
		"microframework",
		"rest",
		"router",
		"psr7"
	],
	"homepage" : "http://github.com/slimphp/Slim-Skeleton",
	"license" : "MIT",
	"authors" : [{
			"name" : "Josh Lockhart",
			"email" : "info@joshlockhart.com",
			"homepage" : "http://www.joshlockhart.com/"
		}
	],
	"require" : {
		"php" : ">=5.5.0",
		"slim/slim" : "^3.1",
		"slim/php-view" : "^2.0",
		"monolog/monolog" : "^1.17"
	},
	"require-dev" : {
		"phpunit/phpunit" : ">=4.8 < 6.0"
	},
	"autoload" : {
		"psr-4" : {
			"BankAccount\\" : "src/BankAccount",
			"BankAccount\\Domain\\services\\" : "src/BankAccount/Domain/services",
			"App" : "app"
		}
	},
	"config" : {
		"process-timeout" : 0
	},
	"scripts" : {
		"start" : "php -S localhost:8080 -t public public/index.php",
		"test" : "phpunit"
	}
}